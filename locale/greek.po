#. COUNT, COUNT, COUNT, COUNT, COUNT, COUNT, COUNT, COUNT
msgctxt "MSG_TR_FILTER_STATS_SPAM (2072//)"
msgid ""
"\n"
"Filtering done:\n"
"\n"
"%3ld messages checked,\n"
"%3ld redirected,\n"
"%3ld forwarded,\n"
"%3ld auto-replied,\n"
"%3ld command activated,\n"
"%3ld moved,\n"
"%3ld deleted,\n"
"%3ld classified as spam"
