msgctxt "MSG_HELP_CO_ST_CTYPE (615//)"
msgid ""
"Enter a new MIME type or select one\n"
"using the popup button at the right."
msgstr "Εισάγετε ένα νέο τύπο MIME ή επιλέξτε ένα\nχρησιμοποιώντας το κουμπί στα δεξιά."

msgctxt "MSG_HELP_CO_ST_COMMAND (616//)"
msgid ""
"Path and optional arguments of\n"
"the viewer command. Click the popup\n"
"button to see the list of supported\n"
"variables."
msgstr "Διαδρομή και προαιρετικές παραμέτρους της\nεντολής αρχείου προβολέα. Κάντε κλικ στο κουμπί\nγια να δείτε ποιές μεταβλητές υποστηρίζονται."

msgctxt "MSG_HELP_CO_BT_MADD (617//)"
msgid "Add a new MIME viewer."
msgstr "Προσθέστε ένα νέο αρχείο προβολέα MIME."

msgctxt "MSG_HELP_CO_BT_MDEL (618//)"
msgid ""
"Remove the selected\n"
"MIME viewer."
msgstr "Αφαιρέστε το επιλεγμένο\nαρχείο προβολέα MIME."

msgctxt "MSG_HELP_CO_ST_EXTENS (619//)"
msgid ""
"A space separated list of extensions\n"
"(suffixes) used by this file type."
msgstr "Μια κενή διαχωρισμένη λίστα των επεκτάσεων\n(επιθέματα) χρησιμοποιούμενη από αυτό τον τύπο αρχείου."

msgctxt "MSG_HELP_CO_ST_DEFVIEWER (620//)"
msgid ""
"Program used to display\n"
"attachments of unknown type."
msgstr "Πρόγραμμα για την χρήση εμφάνισης\nσυνημμένων άγνωστου τύπου."

msgctxt "MSG_HELP_CO_ST_HELLOTEXT (625//)"
msgid ""
"Text to insert at the\n"
"top of a new message."
msgstr "Κείμενο που θα εισάγετε στην\nκορυφή ενός νέου μηνύματος."

msgctxt "MSG_HELP_CO_ST_BYETEXT (626//)"
msgid ""
"Text to add at the\n"
"bottom of a new message."
msgstr "Κείμενο που θα προσθέσετε στο\nτέλος ενός νέου μηνύματος."

msgctxt "MSG_HELP_CO_ST_EDWRAP (628//)"
msgid ""
"Word-wrap message text\n"
"at this column."
msgstr "Κείμενο μηνύματος αναδίπλωσης λέξης\nσε αυτή την στήλη."

msgctxt "MSG_HELP_CO_CY_EDWRAP (630//)"
msgid ""
"Select 'as you type' to have line feeds\n"
"automatically added while you're\n"
"typing your messages."
msgstr "Επιλέξτε 'κατά την πληκτρολόγηση' για να γίνεται αυτόματα\nη αλλαγή γραμμής ενώ τυπώνετε τα μηνύματά σας."

msgctxt "MSG_HELP_CO_CH_LAUNCH (631//)"
msgid ""
"Automatically launch the external editor\n"
"when starting a new message."
msgstr "Αυτόματη εκτέλεση του εξωτερικού κειμενογράφου\nόταν αρχίζετε ένα νέο μήνυμα."

msgctxt "MSG_HELP_CO_ST_FWDSTART (633//)"
msgid ""
"Text used to introduce a forwarded message.\n"
"Click the popup button at the right to see\n"
"the variables you can use."
msgstr "Κείμενο που θα χρησιμοποιείται για την εισαγωγή ενός προωθημένου μηνύματος.\nΚάντε κλικ στο κουμπί δεξιά για να δείτε τις μεταβλητές που μπορείτε να χρησιμοποιήσετε."

msgctxt "MSG_HELP_CO_ST_FWDEND (634//)"
msgid ""
"Text to add at the end\n"
"of a forwarded message."
msgstr "Κείμενο που θα προσθέσετε κατά το τέλος\nενός προωθημένου μηνύματος."

msgctxt "MSG_HELP_CO_ST_REPLYTEXT (635//)"
msgid ""
"Default phrases used in replies to messages. Use the\n"
"popup button at the right to see the supported variables."
msgstr "Προκαθορισμένες φράσεις που θα χρησιμοποιηθούν ως απαντήσεις\nστα μηνύματα. Χρησιμοποιήστε το κουμπί στα δεξιά για να δείτε τις υποστηριζόμενες μεταβλητές."

msgctxt "MSG_HELP_CO_ST_AREPLYTEXT (636//)"
msgid ""
"Use these phrases instead when then pattern\n"
"below matches the recipient's address."
msgstr "Κάντε χρήση αυτών των φράσεων όταν το παρακάτω\nμοτίβο ταιριάζει με την διεύθυνση του παραλήπτη."

msgctxt "MSG_HELP_CO_ST_AREPLYPAT (637//)"
msgid ""
"AmigaDOS pattern that triggers\n"
"the alternative reply phrases."
msgstr "Μοτίβο AmigaDOS που ενεργοποιεί τις\nεναλλακτικές φράσεις απάντησης."

msgctxt "MSG_HELP_CO_ST_MREPLYTEXT (638//)"
msgid ""
"Use these phrases in replies to\n"
"messages from a mailing list."
msgstr "Κάντε χρήση αυτών των φράσεων για την απάντηση\nτων μηνυμάτων από την λίστα αλληλογραφίας."

msgctxt "MSG_HELP_CO_CH_QUOTEEMPTY (641//)"
msgid ""
"Insert quote character\n"
"for empty lines?"
msgstr "Θέλετε να εισάγετε χαρακτήρα παράθεσης\nγια κενές γραμμές;"

msgctxt "MSG_HELP_CO_CH_COMPADDR (642//)"
msgid ""
"If checked, you will be asked where to send your\n"
"reply when the 'From:' and 'Reply-To:' addresses\n"
"of the original message differ."
msgstr "Εάν επιλεχτεί, θα ερωτηθείτε που θέλετε να στείλετε την\nαπάντησή σας όταν οι διευθύνσεις 'Από:' και 'Απάντηση-Προς:'\nτου αρχικού μηνύματος διαφέρουν."

msgctxt "MSG_HELP_CO_CH_STRIPSIG (643//)"
msgid ""
"Avoids quoting the signature\n"
"of the original message."
msgstr "Αποφεύγει την παράθεση της υπογραφής\nτου αρχικού μηνύματος."

msgctxt "MSG_HELP_CO_BT_EDITSIG (645//)"
msgid ""
"Modify this signature\n"
"using the external editor."
msgstr "Τροποποιήστε αυτή την υπογραφή\nχρησιμοποιώντας εξωτερικό κειμενογράφο."

msgctxt "MSG_HELP_CO_BT_INSTAG (646//)"
msgid ""
"Insert a control code\n"
"for a random tagline."
msgstr "Εισάγετε ένα κωδικό ελέγχου\nγια μια τυχαία γραμμή ετικέτας."

msgctxt "MSG_HELP_CO_BT_INSENV (647//)"
msgid ""
"Insert a control code for the\n"
"'SIGNATURE' environment variable."
msgstr "Εισάγετε ένα κωδικό ελέγχου για την\nμεταβλητή περιβάλλοντος 'ENV:SIGNATURE'."

msgctxt "MSG_HELP_CO_ST_TAGFILE (648//)"
msgid ""
"Specify here a database file containing\n"
"taglines (also known as 'cookies')."
msgstr "Καθορίστε εδώ ένα αρχείο βάσης δεδομένων που\nπεριέχει γραμμές ετικέτας (γνωστά ως 'cookies')."

msgctxt "MSG_HELP_CO_ST_TAGSEP (649//)"
msgid ""
"String used to separate the\n"
"taglines in the database."
msgstr "Συμβολοσειρά που θα χρησιμοποιηθεί για τον διαχωρισμό\nτων γραμμών ετικέτας στην βάση δεδομένων."

msgctxt "MSG_HELP_CO_CG_FO (650//)"
msgid ""
"Columns that should be\n"
"displayed in the folder list."
msgstr "Στήλες που θα εμφανίζονται\nστη λίστα φακέλου."

msgctxt "MSG_HELP_CO_CG_MA (651//)"
msgid ""
"Columns that should be\n"
"displayed in the message list."
msgstr "Στήλες που θα εμφανίζονται\nστη λίστα μηνύματος."

msgctxt "MSG_HELP_CO_CG_AB (652//)"
msgid ""
"Columns that should be\n"
"displayed in the address book."
msgstr "Στήλες που θα εμφανίζονται\nστο βιβλίο διευθύνσεων."

msgctxt "MSG_HELP_CO_CH_FIXFLIST (653//)"
msgid ""
"Display list entries\n"
"using a fixed width font."
msgstr "Εμφάνιση καταχωρήσεων λίστας με\nχρήση μιας σταθερής γραμματοσειράς."

msgctxt "MSG_HELP_CO_CH_BEAT (654//)"
msgid ""
"Display time in Swatch Beats, the\n"
"unit of time from Swatch."
msgstr "Εμφάνιση ώρας με την μέθοδο Swatch Beats, την\n επαναστατική μονάδα χρόνου από την Swatch."

msgctxt "MSG_HELP_CO_ST_PGPCMD (655//)"
msgid ""
"Directory where the PGP\n"
"executable can be found."
msgstr "Κατάλογος που μπορεί να βρεθεί\nτο εκτελέσιμο αρχείο PGP."

msgctxt "MSG_HELP_CO_ST_LOGFILE (660//)"
msgid ""
"Path where the log\n"
"files will be stored."
msgstr "Διαδρομή που θα αποθηκευτούν\nτα αρχεία καταγραφής."

msgctxt "MSG_HELP_CO_CH_SPLITLOG (661//)"
msgid ""
"If checked, a separate log file\n"
"will be started every month."
msgstr "Εάν επιλεχτεί, ένα ξεχωριστό αρχείο\nκαταγραφής θα ξεκινάει κάθε μήνα."

msgctxt "MSG_HELP_CO_CY_LOGMODE (662//)"
msgid "Select type of logging."
msgstr "Επιλέξτε τύπο καταγραφής."

msgctxt "MSG_HELP_CO_CH_LOGALL (663//)"
msgid ""
"Disable this option to\n"
"log mail transfers only."
msgstr "Απενεργοποιήστε αυτή την επιλογή για να\nκαταγράφετε μόνο μεταφορές αλληλογραφίας."

msgctxt "MSG_HELP_CO_CH_LOADALL (664//)"
msgid "Load index files of all folders."
msgstr "Φόρτωση αρχείων ευρετηρίου όλων των φακέλων."

msgctxt "MSG_HELP_CO_CH_MARKNEW (665//)"
msgid ""
"Change status of new\n"
"messages to unread."
msgstr "Αλλαγή κατάστασης των νέων\nμηνυμάτων σε μη αναγνωσμένα."

msgctxt "MSG_HELP_CO_CH_SEND (666//)"
msgid "Send queued messages."
msgstr "Αποστολή μηνυμάτων στην ουρά."

msgctxt "MSG_HELP_CO_CH_CHECKBD (668//)"
msgid ""
"Check the address book and open a\n"
"reminder if it's a person's birthday."
msgstr "Έλεγχος του βιβλίου διευθύνσεων και άνοιγμα μιας\nυπενθύμισης εάν είναι τα γενέθλια κάποιας επαφής."

msgctxt "MSG_HELP_CO_CH_DELETEOLD (669//)"
msgid ""
"Delete messages that exceed the\n"
"maximum age (see folder configuration)."
msgstr "Διαγραφή μηνυμάτων που έκαναν υπέρβαση της\nμέγιστης ηλικίας (δείτε ρυθμίσεις φακέλου)."

msgctxt "MSG_HELP_CO_CH_REMOVEDEL (670//)"
msgid "Empty Trash folder."
msgstr "Άδειασμα του κάδου."

msgctxt "MSG_HELP_CO_ST_GALLDIR (671//)"
msgid ""
"Directory to save the pictures\n"
"of your correspondents."
msgstr "Κατάλογος για την αποθήκευση των φωτογραφιών\nτων ανταποκριτών σας."

msgctxt "MSG_HELP_CO_CY_ATAB (673//)"
msgid ""
"Automatically create an address book entry\n"
"for the sender when you read his/her message?"
msgstr "Αυτόματη δημιουργία μιας καταχώρησης στο βιβλίο διευθύνσεων\nγια τον αποστολέα όταν διαβάσετε το μήνυμά του/της;"

msgctxt "MSG_HELP_CO_ST_PROXY (674//)"
msgid ""
"Address and optional port number of your\n"
"HTTP proxy server (default port is 80).\n"
"You may leave this field empty."
msgstr "Διεύθυνση και προαιρετικός αριθμός θύρας του\nδιακομιστή μεσολάβησης HTTP (προκαθορισμένη θύρα είναι 80).\nΜπορείτε να αφήσετε αυτό το πεδίο κενό."

msgctxt "MSG_HELP_CO_ST_NEWGROUP (675//)"
msgid ""
"Add new address book entries to this group\n"
"(it will be created if it doesn't exist yet)."
msgstr "Προσθέστε νέες καταχωρήσεις στο βιβλίο διευθύνσεων για αυτή την ομάδα\n(θα δημιουργηθεί εάν δεν υπάρχει ήδη)."

msgctxt "MSG_HELP_CO_ST_TEMPDIR (676//)"
msgid ""
"Directory to store\n"
"temporary files."
msgstr "Κατάλογος για την αποθήκευση\nπροσωρινών αρχείων."

msgctxt "MSG_HELP_CO_ST_DETACHDIR (677//)"
msgid ""
"Directory to save\n"
"attached files to."
msgstr "Κατάλογος για την αποθήκευση\nσυνημμένων αρχείων."

msgctxt "MSG_HELP_CO_ST_ATTACHDIR (678//)"
msgid ""
"Default source directory\n"
"for attachments."
msgstr "Προκαθορισμένος κατάλογος προέλευσης\nγια τα συνημμένα."

msgctxt "MSG_HELP_CO_ST_APP (679//)"
msgid ""
"Try to put the AppIcon\n"
"exactly at these coordinates."
msgstr "Προσπάθεια τοποθέτησης του AppIcon\nακριβώς σε αυτές τις συντεταγμένες."

msgctxt "MSG_HELP_CO_CH_CLGADGET (680//)"
msgid ""
"Do you want YAM to iconify when\n"
"you hit the close gadget?"
msgstr "Θέλετε το YAM να κάνει ελαχιστοποίηση όταν\nπατήσετε το κουμπί κλεισίματος παραθύρου;"

msgctxt "MSG_HELP_CO_CH_CONFIRM (681//)"
msgid ""
"Do you want to confirm\n"
"deletion of messages?"
msgstr "Θέλετε να κάνετε επιβεβαίωση\nδιαγραφής των μηνυμάτων;"

msgctxt "MSG_HELP_CO_NB_CONFIRMDEL (682//)"
msgid ""
"Minimum number of messages to\n"
"display the confirmation requester."
msgstr "Ελάχιστος αριθμός μηνυμάτων που θα\nεμφανίσουν την αίτηση επιβεβαίωσης."

msgctxt "MSG_HELP_CO_CH_REMOVE (683//)"
msgid ""
"Immediately delete the messages rather\n"
"than moving them to the Trash folder.\n"
"(Use this with care)"
msgstr "Άμεση διαγραφή των μηνυμάτων παρά την\nμετακίνησή τους στον κάδο.\n(Χρησιμοποιήστε το με προσοχή)"

msgctxt "MSG_HELP_CO_TX_ENCPACK (690//)"
msgid ""
"XPK library used to protect\n"
"and compress folders."
msgstr "Χρήση της βιβλιοθήκης XPK για την προστασία\nκαι την συμπίεση των φακέλων."

msgctxt "MSG_HELP_CO_TX_PACKER (691//)"
msgid ""
"XPK library used to\n"
"compress folders."
msgstr "Χρήση της βιβλιοθήκης XPK για την\nσυμπίεση των φακέλων."

msgctxt "MSG_HELP_CO_NB_ENCPACK (692//)"
msgid ""
"Efficiency of this packer. 100% is\n"
"the best value, but also the slowest."
msgstr "Απόδοση του συμπιεστή. 100% είναι η\nκαλύτερη τιμή, αλλά επίσης και η βραδύτερη."

msgctxt "MSG_HELP_CO_ST_ARCHIVER (693//)"
msgid ""
"Command and options used to\n"
"create archived attachments."
msgstr "Εντολή και επιλογές που θα χρησιμοποιηθούν\nγια την δημιουργία αρχειοθετημένων συνημμένων."

msgctxt "MSG_HELP_CO_ST_RXNAME (694//)"
msgid ""
"Description of the script\n"
"in the 'Scripts' menu."
msgstr "Περιγραφή της δέσμης εντολών\nστο μενού 'Δέσμη εντολών'."

msgctxt "MSG_HELP_CO_ST_SCRIPT (695//)"
msgid "Filename of the script."
msgstr "Όνομα αρχείου της δέσμης εντολών."

msgctxt "MSG_HELP_CO_CY_ISADOS (696//)"
msgid ""
"Type of the script\n"
"(ARexx or AmigaDOS)."
msgstr "Τύπος της δέσμης εντολών.\n(ARexx ή AmigaDOS)."

msgctxt "MSG_HELP_CO_CH_CONSOLE (697//)"
msgid ""
"Open a console window to allow input\n"
"and output from the script."
msgstr "Άνοιγμα ενός παραθύρου κονσόλας για την αποδοχή\nεισαγωγής και εξαγωγής από την δέσμη εντολών."

msgctxt "MSG_HELP_CO_CH_WAITTERM (698//)"
msgid ""
"Do not run the script\n"
"asynchronously but wait\n"
"for it to complete."
msgstr "Μην τρέξετε την δέσμη εντολών\nασύγχρονα, αλλά περιμένετε\nμέχρι να τελειώσει."

msgctxt "MSG_HELP_CO_BT_SAVE (699//)"
msgid "Save current settings."
msgstr "Αποθηκεύστε τις τρέχουσες ρυθμίσεις."

msgctxt "MSG_HELP_CO_BT_USE (700//)"
msgid "Use current settings."
msgstr "Χρησιμοποιήστε τις τρέχουσες ρυθμίσεις."

msgctxt "MSG_HELP_CO_BT_CANCEL (701//)"
msgid ""
"Close configuration\n"
"window and use the old\n"
"settings."
msgstr "Κλείστε το παράθυρο ρυθμίσεων\nκαι χρησιμοποιήστε τις παλιές επιλογές."

msgctxt "MSG_AB_BirthdayReminder (703//)"
msgid "Birthday Reminder"
msgstr "Υπενθύμιση γενεθλίων"

msgctxt "MSG_AB_HappyBirthday (704//)"
msgid "Happy birthday"
msgstr "Ευτυχισμένα γενέθλια!"

