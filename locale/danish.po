msgctxt "MSG_CO_ScriptPreWriteMsg (538//)"
msgid "Before writing a message"
msgstr "Før skrivning af meddelelse"

msgctxt "MSG_CO_ScriptPostWriteMsg (539//)"
msgid "After writing a message"
msgstr "Efter skrivning af meddelelse"

msgctxt "MSG_CO_ScriptClickURL (540//)"
msgid "When double-clicking an URL"
msgstr "Ved dobbeltklik på en URL"

