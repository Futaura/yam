<?xml version="1.0" encoding="iso-8859-1"?>
<!DOCTYPE library SYSTEM "library.dtd">
<library name="cmanager" basename="CManagerBase" openname="cmanager.library">
	<include>exec/types.h</include>
	<interface name="main" version="1.0" struct="CManagerIFace" prefix="_CManager_" asmprefix="ICManager" global="ICManager">
		<method name="Obtain" result="ULONG"/>
		<method name="Release" result="ULONG"/>
		<method name="Expunge" result="void" status="unimplemented"/>
		<method name="Clone" result="struct Interface *" status="unimplemented"/>
		<method name="CM_StartManager" result="APTR">
			<arg name="par1" type="STRPTR"/>
			<arg name="last" type="STRPTR"/>
		</method>
		<method name="CM_LoadData" result="BOOL">
			<arg name="par1" type="STRPTR"/>
			<arg name="par2" type="struct CMData *"/>
			<arg name="last" type="STRPTR"/>
		</method>
		<method name="CM_SaveData" result="void">
			<arg name="par1" type="STRPTR"/>
			<arg name="par2" type="struct CMData *"/>
			<arg name="last" type="STRPTR"/>
		</method>
		<method name="CM_FreeData" result="void">
			<arg name="last" type="struct CMData *"/>
		</method>
		<method name="CM_GetParent" result="struct CMGroup *">
			<arg name="par1" type="struct CMGroup *"/>
			<arg name="last" type="struct CMGroup *"/>
		</method>
		<method name="CM_AllocEntry" result="APTR">
			<arg name="last" type="ULONG"/>
		</method>
		<method name="CM_FreeEntry" result="void">
			<arg name="last" type="APTR"/>
		</method>
		<method name="CM_FreeHandle" result="void">
			<arg name="par1" type="APTR"/>
			<arg name="last" type="BOOL"/>
		</method>
		<method name="CM_GetEntry" result="APTR">
			<arg name="par1" type="APTR"/>
			<arg name="last" type="ULONG"/>
		</method>
		<method name="CM_CreateBitMap" result="struct BitMap *">
			<arg name="par1" type="ULONG"/>
			<arg name="par2" type="ULONG"/>
			<arg name="par3" type="ULONG"/>
			<arg name="par4" type="ULONG"/>
			<arg name="last" type="struct BitMap *"/>
		</method>
		<method name="CM_DeleteBitMap" result="void">
			<arg name="last" type="struct BitMap *"/>
		</method>
		<method name="CM_AddEntry" result="BOOL">
			<arg name="last" type="APTR"/>
		</method>
		<method name="CM_FreeList" result="void">
			<arg name="last" type="struct MinList *"/>
		</method>
	</interface>
</library>
